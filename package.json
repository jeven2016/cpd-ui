  "dependencies": {
    "@react-keycloak/web": "^3.4.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.1",
    "@types/node": "^16.7.13",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "antd": "^4.22.3",
    "axios": "^0.24.0",
    "connected-react-router": "^6.9.2",
    "core-js": "^3.20.1",
    "dayjs": "^1.10.7",
    "i18next": "^21.8.14",
    "i18next-browser-languagedetector": "^6.1.4",
    "i18next-http-backend": "^1.4.1",
    "keycloak-js": "^19.0.1",
    "loglevel": "^1.8.0",
    "qrcode.react": "^1.0.1",
    "react": "^17.0.2",
    "react-app-polyfill": "^3.0.0",
    "react-cookies": "^0.1.1",
    "react-dom": "^17.0.2",
    "react-i18next": "^11.18.1",
    "react-intl-universal": "^2.4.8",
    "react-redux": "^7.2.6",
    "react-router-dom": "^6.2.1",
    "react-scripts": "5.0.0",
    "react-windy-ui": "^0.7.1-rc8",
    "redux": "^4.1.2",
    "redux-logger": "^3.0.6",
    "redux-saga": "^1.1.3",
    "redux-thunk": "^2.4.1",
    "typescript": "^4.4.2",
    "web-vitals": "^2.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@craco/craco": "^6.4.3",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "circular-dependency-plugin": "^5.2.2",
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "^2.2.2",
    "craco-plugin-react-hot-reload": "^0.1.0",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.4",
    "lodash": "^4.17.21",
    "prettier": "^2.5.1",
    "react-hot-loader": "^4.13.0",
    "sass": "^1.45.1",
    "serve": "^13.0.2",
    "simple-progress-webpack-plugin": "^2.0.0",
    "webpack-bundle-analyzer": "^4.5.0"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=development craco start ",
    "build": "cross-env NODE_ENV=production craco build",
    "preview": "serve -s build",
    "test": "cross-env NODE_ENV=development craco test",
    "eject": "react-scripts eject",
    "lint": "eslint ./src",
    "lint:fix": "eslint --fix ./src",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,scss,md,json}' --config ./.prettierrc",
    "prepare": "husky install",
    "genlog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "commit": "git-cz"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "src/**/*.{js,jsx,ts,tsx,css,scss,md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
